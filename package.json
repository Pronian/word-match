		"@sveltejs/kit": "^1.22.2",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.32.2",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.6",
		"tslib": "^2.6.0",
		"typescript": "^5.1.6",
		"unplugin-icons": "^0.16.3",
		"vite": "^4.4.3"
