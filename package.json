		"@typescript-eslint/parser": "^6.12.0",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.0",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.6.2",
		"typescript": "^5.3.2",
